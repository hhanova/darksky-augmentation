{
    "name": "keboola/foreacastio-extractor-bundle",
    "type": "symfony-bundle",
    "description": "Keboola Forecast.io Weather Extractor Bundle",
    "keywords": ["keboola", "weather", "forecast.io"],
    "authors": [
        {
            "name": "Jakub Matejka",
            "email": "jakub@keboola.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/extractor-bundle.git"
        }
    ],
    "require": {
        "syrup/component-bundle": "~1.7.0",
        "willdurand/geocoder": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "Keboola\\ForecastIoExtractorBundle": "" }
    },
    "target-dir": "Keboola/ForecastIoExtractorBundle",
    "extra": {
        "symfony-app-dir": "vendor/keboola/syrup/app",
        "symfony-web-dir": "vendor/keboola/syrup/web",
        "incenteev-parameters": {
            "keep-outdated": true,
            "file": "vendor/keboola/syrup/app/config/parameters.yml",
            "dist-file": "parameters.yml"
        }
    }
}