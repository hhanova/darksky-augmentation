{
    "name": "keboola/forecastio-augmentation",
    "license": "MIT",
    "description": "Keboola Forecast.io Weather Augmentation",
    "keywords": ["keboola", "weather", "forecast.io"],
    "authors": [
        {
            "name": "Jakub Matejka",
            "email": "jakub@keboola.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "CNG/ForecastTools",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/CNG/ForecastTools",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["lib"]
                }
            }

        }
    ],
    "require": {
        "keboola/csv": "~1.1.3",
        "symfony/process": "~3.0.1",
        "doctrine/dbal": "v2.5.4",
        "symfony/yaml": "~2.6.3",
        "CNG/ForecastTools": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "phpunit/php-code-coverage": "^3",
        "squizlabs/php_codesniffer": "~2.5",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Keboola\\": "src/Keboola/"
        }
    }
}
