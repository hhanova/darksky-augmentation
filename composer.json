{
    "name": "keboola/forecastio-augmentation",
    "type": "symfony-bundle",
    "description": "Keboola Forecast.io Weather Augmentation",
    "keywords": ["keboola", "weather", "forecast.io"],
    "authors": [
        {
            "name": "Jakub Matejka",
            "email": "jakub@keboola.com"
        }
    ],
    "repositories": [],
    "require": {
        "keboola/syrup": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "symfony/var-dumper": "~2.6",
        "squizlabs/php_codesniffer": "2.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Keboola\\ForecastIoAugmentation": ""
        }
    },
    "target-dir": "Keboola/ForecastIoAugmentation",
    "extra": {
        "symfony-app-dir": "vendor/keboola/syrup/app",
        "symfony-web-dir": "vendor/keboola/syrup/web",
        "syrup-app-name": "ag-forecastio"
    }
}